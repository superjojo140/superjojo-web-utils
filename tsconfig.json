{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
  },
  "include": [
    "module_generator/module_generator.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}